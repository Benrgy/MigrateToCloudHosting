{
  "rewrites": [
    {
      "source": "/calculator",
      "destination": "/index.html"
    },
    {
      "source": "/thank-you",
      "destination": "/index.html"
    },
    {
      "source": "/blog",
      "destination": "/index.html"
    },
    {
